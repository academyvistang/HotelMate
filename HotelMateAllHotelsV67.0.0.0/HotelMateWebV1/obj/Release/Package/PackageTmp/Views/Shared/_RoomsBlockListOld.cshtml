@model HotelMateWebV1.Models.SearchViewModel

<link href="~/css/CoolTable.css" rel="stylesheet" />

<div id="hero" class="row">
    <div class="twelve columns">

        <div>
            <table class="striped fixedlayout">
                <thead>
                    <tr>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                    </tr>
                </thead>
                <tbody id="CasesTableBody1">
                    @{ int i = 0; }
                    @while (i < Model.GuestRoomsList.Count)
                    {
                        var max = 6;
                        <tr>
                            @for (var p = 0; p < max; p++)
                            {
                                if (i < Model.GuestRoomsList.Count)
                                {
                                    <td class="tth2">
                                        <a , title="@Model.GuestRoomsList[i].Guest.FullName" class="erbEditorOpener buttonDevFirst" href="@Url.Action("EditSearchBooking", "Booking", new { id = Model.GuestRoomsList[i].Room.Id })">Model.GuestRoomsList[i].Room.RoomNumber</a>
                                    </td>
                                    i++;
                                }
                            }
                        </tr>
                    }

                </tbody>
            </table>

        </div>


    </div> <!-- #hero -->

</div>

