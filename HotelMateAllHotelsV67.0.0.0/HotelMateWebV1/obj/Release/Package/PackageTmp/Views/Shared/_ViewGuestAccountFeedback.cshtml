@using System.Globalization
@using HotelMateWebV1.Helpers
@model HotelMateWebV1.Models.GuestRoomAccountViewModel

<link href="~/css/CoolTable.css" rel="stylesheet" />

<div class="twelve columns">
    <h1 class="page-title ci-title" style="margin-bottom:10px;"><span>Guest Feedback</span></h1>

@using (Html.BeginForm("GuestFeedback", "Guest", FormMethod.Post, new { enctype = "multipart/form-data", @id="target", @class = "booking-form b-form row" }))
                {

    <div class="twelve columns">
        <label for="comments">Comments</label>
        @Html.TextAreaFor(model => model.Feedback, new { @cols = "170", @rows = "10" })        
    </div>

    <div class="twelve columns">
    <input id="submit" type="submit" value="Submit" name="send_booking">
    </div>
}




  
</div>












